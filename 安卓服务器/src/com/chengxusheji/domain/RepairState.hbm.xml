<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.chengxusheji.domain">

	<class name="RepairState" table="RepairState">

          <!-- 主键状态id -->
          <id name="repairStateId" column="repairStateId">
            <generator class="native"/>
          </id>

          <!-- 简单字符串属性: 状态名称 -->
          <property
            name="repairStateName"
            column="repairStateName"
            type="string"
            length="20"
          />


		 
		
	</class>
	
</hibernate-mapping>
 
